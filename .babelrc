{
  "plugins": [
    "babel-plugin-styled-components",
    ["babel-plugin-root-import", {
      "rootPathSuffix": "./"
    }]
  ],
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
